  "private": true,
  "dependencies": {
    "@applitools/eyes-cypress": "^3.12.0",
    "axios": "^0.21.1",
    "cross-env": "^7.0.2",
    "cypress": "^4.8.0",
    "cypress-dotenv": "^1.2.1",
    "dotenv-flow": "^3.1.0",
    "jsonpath": "^1.0.2",
    "prettier-standard": "^16.3.0"
  }
}
