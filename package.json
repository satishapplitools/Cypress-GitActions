{
  "name": "content-tester",
  "version": "1.0.0",
  "description": "Content Tests",
  "scripts": {
    "cypress:local": "cross-env NODE_ENV=dev node_modules/.bin/cypress open",
    "cypress:ci": "node_modules/.bin/cypress run",
    "lint": "node_modules/.bin/prettier-standard --lint",
    "format": "node_modules/.bin/prettier-standard --format",
